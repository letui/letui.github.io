;(function($,window){$("#form_data .block").each(function(index,item){var component_type=$(this).attr("component-type");switch(component_type){case"105":resetHeight.call(this);break;case"106":resetHeight.call(this);break}})})($,window);function resetHeight(){var list=$(this).find(".optionList");if(list.hasClass("two-columns")){twoColumns.call(this)}else if(list.hasClass("three-columns")){threeColumns.call(this)}else{oneColumns.call(this)}}function oneColumns(){var target=$(this).find(".array-item");target.each(function(index,item){if($(this).find("div.pic").length>0){$(this).css("padding-top","0")}})}function twoColumns(){var target=$(this).find(".array-item");var len=Math.floor(target.length/2);for(var i=1;i<len+1;i++){if(target.eq(2*i-1).find("div.pic").length&&target.eq(2*i-1).prev().find("div.pic").length){console.log(target.eq(2*i-1));target.eq(2*i-1).find("div.pic").css("display","none");target.eq(2*i-1).prev().find("div.pic").css("display","none");target.eq(2*i-1).css("padding-top","0");target.eq(2*i-1).prev().css("padding-top","0")}}if(i*2!=target.length){target.eq(target.length-1).find("div.pic").css("display","none")}}function threeColumns(){var target=$(this).find(".array-item");var allLen=target.length;var len=Math.floor(target.length/3);for(var i=1;i<len+1;i++){if(target.eq(3*i-1).find("div.pic").length&&target.eq(3*i-1).prev().find("div.pic").length&&target.eq(3*i-2).prev().find("div.pic").length){target.eq(3*i-1).find("div.pic").css("display","none");target.eq(3*i-1).prev().find("div.pic").css("display","none");target.eq(3*i-2).prev().find("div.pic").css("display","none");target.eq(3*i-1).css("padding-top","0");target.eq(3*i-1).prev().css("padding-top","0");target.eq(3*i-2).prev().css("padding-top","0")}}if(allLen===len*3+1&&target.eq(allLen-1).find("div.pic").length){target.eq(allLen-1).find("div.pic").css("display","none")}else if(allLen===len*3+2&&target.eq(allLen-1).find("div.pic").length&&target.eq(allLen-2).find("div.pic").length){target.eq(allLen-1).find("div.pic").css("display","none");target.eq(allLen-2).find("div.pic").css("display","none")}}